// program to print given number is Armstrong number or not
#include<stdio.h>
#include<conio.h>
void main()
{
int n,r,sum=0,temp;
clrscr();
printf("\n enter the numbers");
scanf("%d\n",&n);
temp=n;
while(n>0)
{
r=n%10;
sum=sum+(r*r*r);
n=n/10;
}
if(temp==sum)
printf("%d\n is armstrong number");
else
printf("%d\n is not a armstrong number");
getch();
}
